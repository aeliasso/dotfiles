[user]
	name = Anton Eliasson
	email = devel@antoneliasson.se
[alias]
	co = checkout
	ci = commit
	st = status
	br = branch
	hist = log --pretty=format:'%h %ad | %s%d [%an]' --graph --date=short
	type = cat-file -t
	dump = cat-file -p
[push]
	default = upstream
[core]
	pager = less -F
[pull]
	ff = only
[rebase]
	autoSquash = true
